{
  "name": "storeshot-studio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "test": "node --import tsx --test tests/*.test.js",
    "desktop:dev": "npm --prefix apps/desktop run dev",
    "desktop:build": "npm --prefix apps/desktop run build"
  },
  "devDependencies": {
    "tsx": "^4.21.0"
  }
}
